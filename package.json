  },
  "homepage": "https://github.com/spsprinkles/message-center#readme",
  "dependencies": {
    "datatables.net": "^1.13.11",
    "datatables.net-bs5": "^1.13.11",
    "dattatable": "^2.10.0",
    "gd-sprest-bs": "^10.11.52",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
